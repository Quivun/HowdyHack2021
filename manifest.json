{
    "name": "MAHJ",
    "author" : "Aaron Su, Harsh Gangaramani, Joshua Chong, and Min Zhang",
    "description": "HowdyHack 2021 project for an interactive audio player overlay for daily usage",
    "homepage_url": "https://github.com/Quivun/HowdyHack2021",
    "version": "0.0.1",
    "manifest_version": 2,
    "offline_enabled": true,
    "permissions": [ "storage", "tabs", "activeTab", "http://*/", "https://*/" ],
    "browser_action": {
        "default_icon": "images/AS.png",
        "default_popup": "howdy.html",
        "default_title": "MAHJ"
        
    },
    "background": {
        "scripts": [
            "scripts/background.js"
        ]
    },
    "content_scripts": [
        {
            "all_frames": false,
            "css":[ ],
            "matches": [
                "http://*/*", "https://*/*","<all_urls>"
            ],
            "run_at": "document_idle",
            "js": [
                "scripts/jquery-3.6.0.min.js",
                "scripts/content.js"
            ]
        }
    ]

}